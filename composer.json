{
    "name": "joomla/oauth1",
    "type": "joomla-package",
    "description": "Joomla OAuth1 Package",
    "keywords": ["joomla", "framework", "oauth1"],
    "homepage": "https://github.com/joomla-framework/oauth1",
    "license": "GPL-2.0+",
    "require": {
        "php": ">=5.3.10",
        "joomla/application": "~1.0",
        "joomla/http": "~1.0",
        "joomla/input": "~1.0",
        "joomla/registry": "~1.0"
    },
    "require-dev": {
        "joomla/test": "~1.0",
        "joomla/event": "~1.0",
        "phpunit/phpunit": "4.*",
        "squizlabs/php_codesniffer": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\OAuth1\\": "src/",
            "Joomla\\OAuth1\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
	        "dev-2.0-dev": "2.0-dev"
        }
    }
}
